{
  "feedbackSurveyState": {
    "lastShownTime": 1754085923371
  },
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "agents": {
    "enabled": true,
    "orchestrator": {
      "enabled": true,
      "autoInvoke": true,
      "triggers": [
        "all_requests"
      ]
